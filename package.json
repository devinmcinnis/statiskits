{
  "name": "ck",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon --inspect ./bin/www & npm run styles:dev",
    "styles:dev": "./node_modules/stylus/bin/stylus --watch --sourcemap ./public/stylesheets/style.styl --out ./public/stylesheets/",
    "styles:build": "./node_modules/stylus/bin/stylus ./public/stylesheets/style.styl --out ./public/stylesheets/style.css --compress"
  },
  "dependencies": {
    "body-parser": "~1.17.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.3",
    "express": "~4.15.2",
    "forever": "^0.15.3",
    "jade": "~1.11.0",
    "moment": "^2.19.2",
    "morgan": "~1.8.1",
    "nodemon": "^1.12.1",
    "request": "^2.83.0",
    "serve-favicon": "~2.4.2",
    "stylus": "^0.54.5"
  }
}
